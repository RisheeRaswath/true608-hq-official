{
  "rewrites": [
    {
      "source": "/app/assets/:path*",
      "destination": "/app/assets/:path*"
    },
    {
      "source": "/app/(.*)",
      "destination": "/app/index.html"
    }
  ],
  "headers": [
    {
      "source": "/app/assets/(.*).js",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" }
      ]
    }
  ]
}